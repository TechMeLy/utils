{
  "name": "@techmely/utils",
  "version": "0.0.3",
  "description": "Collection of helpful JavaScript / TypeScript utils",
  "author": "Harry Tran <nhattq.coding@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/TechMeLy/techmely/utils.git"
  },
  "engines": {
    "node": ">=14.17.6",
    "npm": ">=7.22.0"
  },
  "source": "src/index.ts",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "typings"
  ],
  "scripts": {
    "build": "NODE_ENV=production tsup --dts",
    "clean": "rm -rf dist",
    "prepublish": "yarn clean",
    "version": "yarn version"
  },
  "peerDependencies": {
    "dayjs": "1.10.6",
    "jwt-decode": "3.1.2",
    "throttle-debounce": "3.0.1"
  },
  "peerDependenciesMeta": {
    "dayjs": {
      "optional": true
    },
    "jwt-decode": {
      "optional": true
    },
    "throttle-debounce": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/throttle-debounce": "2.1.0",
    "cross-env": "7.0.3",
    "dayjs": "1.10.7",
    "jwt-decode": "3.1.2",
    "throttle-debounce": "3.0.1"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "funding": [
    {
      "type": "ko-fi",
      "url": "https://ko-fi.com/harrytran998"
    },
    {
      "type": "patreon",
      "url": "https://www.patreon.com/harrytran998"
    }
  ]
}
