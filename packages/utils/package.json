{
  "name": "@techmely/utils",
  "version": "1.1.8",
  "description": "Collection of helpful JavaScript / TypeScript utils",
  "author": "Harry Tran <nhattq.coding@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/techmely/techmely/tree/main/packages/utils"
  },
  "source": "src/index.ts",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsup --dts",
    "build:dev": "tsup",
    "dev": "tsup --watch",
    "clean": "rimraf dist",
    "test": "vitest",
    "coverage": "vitest --coverage"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "peerDependencies": {
    "dayjs": "1.10.6",
    "exceljs": "4.3.0",
    "jwt-decode": "3.1.2"
  },
  "peerDependenciesMeta": {
    "dayjs": {
      "optional": true
    },
    "exceljs": {
      "optional": true
    },
    "jwt-decode": {
      "optional": true
    }
  },
  "devDependencies": {
    "@techmely/build-configs": "workspace:*",
    "@techmely/types": "workspace:*",
    "chalk": "5.0.1",
    "dayjs": "1.11.1",
    "exceljs": "4.3.0",
    "jwt-decode": "3.1.2",
    "tsup": "6.1.2",
    "typescript": "4.6.3",
    "vitest": "0.10.0"
  },
  "funding": [
    {
      "type": "ko-fi",
      "url": "https://ko-fi.com/harrytran998"
    },
    {
      "type": "patreon",
      "url": "https://www.patreon.com/harrytran998"
    }
  ]
}
