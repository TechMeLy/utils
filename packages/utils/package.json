      "./compat": [
        "dist/compat.d.ts"
      ],
      "./generate": [
        "dist/generate.d.ts"
      ],
      "./appMetadata": [
        "dist/appMetadata.d.ts"
      ],
      "./hasProp": [
        "dist/hasProp.d.ts"
      ],
      "./validators": [
        "dist/validators.d.ts"
      ],
      "./array": [
        "dist/array.d.ts"
      ],
      "./date": [
        "dist/date.d.ts"
      ],
      "./number": [
        "dist/number.d.ts"
      ],
      "./object": [
        "dist/object.d.ts"
      ],
      "./string": [
        "dist/string.d.ts"
      ],
      "./vector": [
        "dist/vector.d.ts"
      ]
    }
