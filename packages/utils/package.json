    "exceljs": {
      "optional": true
    },
    "jwt-decode": {
      "optional": true
    }
  },
  "devDependencies": {
    "@techmely/build-configs": "workspace:^",
    "@types/debug": "4.1.7",
    "@types/jest": "27.0.3",
    "chalk": "5.0.0",
    "dayjs": "1.10.7",
    "exceljs": "4.3.0",
    "jwt-decode": "3.1.2",
    "typescript": "4.5.4"
  },
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "funding": [
