{
  "name": "@techmely/utils",
  "version": "1.1.4",
  "description": "Collection of helpful JavaScript / TypeScript utils",
  "author": "Harry Tran <nhattq.coding@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/techmely/techmely/tree/main/packages/utils"
  },
  "source": "src/index.ts",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsup --dts",
    "build:dev": "tsup",
    "dev": "tsup --watch",
    "clean": "rimraf dist",
    "test": "vitest",
    "coverage": "vitest --coverage"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./files": {
      "import": "./dist/files/index.mjs",
      "require": "./dist/files/index.js"
    },
    "./helpers": {
      "import": "./dist/helpers/index.mjs",
      "require": "./dist/helpers/index.js"
    },
    "./is": {
      "import": "./dist/is/index.mjs",
      "require": "./dist/is/index.js"
    },
    "./math": {
      "import": "./dist/math/index.mjs",
      "require": "./dist/math/index.js"
    },
    "./transform": {
      "import": "./dist/transform/index.mjs",
      "require": "./dist/transform/index.js"
    },
    "./services": {
      "import": "./dist/services/index.mjs",
      "require": "./dist/services/index.js"
    },
    "./dayjs": {
      "import": "./dist/vendor/dayjs.mjs",
      "require": "./dist/vendor/dayjs.js"
    },
    "./excel": {
      "import": "./dist/vendor/excel.mjs",
      "require": "./dist/vendor/excel.js"
    },
    "./jwt": {
      "import": "./dist/vendor/jwt.mjs",
      "require": "./dist/vendor/jwt.js"
    },
    "./colors": {
      "import": "./dist/colors.mjs",
      "require": "./dist/colors.js"
    },
    "./compat": {
      "import": "./dist/compat.mjs",
      "require": "./dist/compat.js"
    },
    "./generate": {
      "import": "./dist/generate.mjs",
      "require": "./dist/generate.js"
    },
    "./appMetadata": {
      "import": "./dist/appMetadata.mjs",
      "require": "./dist/appMetadata.js"
    },
    "./hasProp": {
      "import": "./dist/hasProp.mjs",
      "require": "./dist/hasProp.js"
    },
    "./validators": {
      "import": "./dist/validators.mjs",
      "require": "./dist/validators.js"
    },
    "./array": {
      "import": "./dist/array.mjs",
      "require": "./dist/array.js"
    },
    "./date": {
      "import": "./dist/date.mjs",
      "require": "./dist/date.js"
    },
    "./number": {
      "import": "./dist/number.mjs",
      "require": "./dist/number.js"
    },
    "./object": {
      "import": "./dist/object.mjs",
      "require": "./dist/object.js"
    },
    "./string": {
      "import": "./dist/string.mjs",
      "require": "./dist/string.js"
    },
    "./vector": {
      "import": "./dist/vector.mjs",
      "require": "./dist/vector.js"
    }
  },
  "typesVersions": {
    "*": {
      "./files": [
        "dist/files/index.d.ts"
      ],
      "./helpers": [
        "dist/helpers/index.d.ts"
      ],
      "./is": [
        "dist/is/index.d.ts"
      ],
      "./math": [
        "dist/math/index.d.ts"
      ],
