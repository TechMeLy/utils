{
  "name": "@techmely/utils",
  "version": "1.3.2",
  "description": "Collection of helpful JavaScript / TypeScript utils",
  "author": "Harry Tran <nhattq.coding@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/techmely/utils/tree/dev/packages/utils"
  },
  "source": "src/index.ts",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsup --dts",
    "build:dev": "tsup",
    "dev": "tsup --watch",
    "clean": "rimraf dist"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./fs": {
      "import": "./dist/files/fs.mjs",
      "require": "./dist/files/fs.js"
    },
    "./findNearestFile": {
      "import": "./dist/files/findNearestFile.mjs",
      "require": "./dist/files/findNearestFile.js"
    },
    "./git": {
      "import": "./dist/git.mjs",
      "require": "./dist/git.js"
    }
  },
  "typesVersions": {
    "*": {
      ".": [
        "dist/index.d.ts"
      ],
      "fs": [
        "dist/files/fs.d.ts"
      ],
      "findNearestFile": [
        "dist/files/findNearestFile.d.ts"
      ],
      "git": [
        "dist/git.d.ts"
      ]
    }
  },
  "peerDependencies": {
    "dayjs": "^1.11.3"
  },
  "peerDependenciesMeta": {
    "dayjs": {
      "optional": true
    },
    "exceljs": {
      "optional": true
    }
  },
  "devDependencies": {
    "@techmely/build-configs": "workspace:*",
    "@techmely/types": "workspace:*",
    "chalk": "^5.0.1",
    "dayjs": "^1.11.3",
    "tsup": "^6.1.3",
    "typescript": "^4.7.4",
    "vitest": "^0.18.0",
    "execa": "^6.1.0"
  },
  "funding": [
    {
      "type": "ko-fi",
      "url": "https://ko-fi.com/harrytran998"
    },
    {
      "type": "patreon",
      "url": "https://www.patreon.com/harrytran998"
    }
  ]
}
