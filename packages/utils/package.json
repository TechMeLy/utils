  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/TechMeLy/techmely/utils.git"
  },
  "engines": {
    "node": ">=16.13.0",
    "npm": ">=8.1.0"
  },
  "source": "src/index.ts",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
