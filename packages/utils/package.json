  "devDependencies": {
    "@techmely/build-configs": "workspace:*",
    "@techmely/types": "workspace:*",
    "chalk": "5.0.1",
    "dayjs": "1.11.1",
    "exceljs": "4.3.0",
    "jwt-decode": "3.1.2",
    "tsup": "5.12.6",
    "typescript": "4.6.3",
    "vitest": "0.10.0"
  },
  "funding": [
    {
      "type": "ko-fi",
      "url": "https://ko-fi.com/harrytran998"
