{
  "projectType": "library",
  "root": "packages/types",
  "sourceRoot": "packages/types/src",
  "prefix": "techmely",
  "targets": {
    "eslint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": ["packages/utils/src/**/*.ts"],
        "cache": true,
        "quiet": true
      },
      "outputs": ["{options.outputFile}"]
    },
    "build": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "commands": [
          {
            "command": "cd packages/types && npx -y tsup --dts"
          }
        ],
        "cwd": "."
      }
    }
  },
  "tags": ["scope:shared"]
}
