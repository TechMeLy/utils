{
  "name": "@techmely/build-configs",
  "version": "1.0.5",
  "description": "Techmely build configurations",
  "author": "Harry Tran <nhattq.coding@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/techmely/techmely/tree/main/packages/build-configs"
  },
  "engines": {
    "node": ">=16.13.0",
    "npm": ">=8.1.0"
  },
  "source": "src/index.ts",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsup --dts",
    "build:dev": "tsup",
    "dev": "tsup --watch",
    "clean": "rimraf dist"
  },
  "peerDependencies": {
    "tsup": "^5.12.5"
  },
  "devDependencies": {
    "tsup": "^6.0.1"
  },
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "funding": [
    {
      "type": "ko-fi",
      "url": "https://ko-fi.com/harrytran998"
    },
    {
      "type": "patreon",
      "url": "https://www.patreon.com/harrytran998"
    }
  ],
  "bugs": {
    "url": "https://github.com/techmely/techmely/issues"
  },
  "keywords": [
    "techmely",
    "types"
  ]
