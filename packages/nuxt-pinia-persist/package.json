{
  "name": "@techmely/nuxt-pinia-persist",
  "version": "1.0.9",
  "description": "Pinia Persist like Redux-Persist for Nuxt 3",
  "repository": "https://github.com/techmely/essential-packages/tree/main/packages/nuxt-pinia-persist",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": ["dist"],
  "scripts": {
    "build": "nuxt prepare playground && nuxt-module-build build",
    "dev": "nuxt dev playground --dotenv .env.test",
    "dev.build": "nuxt build playground",
    "clean": "rimraf dist",
    "upgrade.deps": "npm-check-updates -u",
    "publish": "npm publish"
  },
  "dependencies": {
    "@nuxt/kit": "3.10.2",
    "@techmely/utils": "^2.1.8",
    "@techmely/vue-pinia-persist": "^1.0.8",
    "defu": "^6.1.4"
  },
  "peerDependencies": {
    "pinia": "^2.1.0"
  },
  "devDependencies": {
    "@nuxt/devtools": "^1.0.8",
    "@nuxt/kit": "3.10.2",
    "@nuxt/module-builder": "^0.5.5",
    "@nuxt/schema": "^3.10.2",
    "@nuxt/test-utils": "3.11.0",
    "@types/node": "20.11.19",
    "nuxt": "^3.10.2",
    "pinia": "^2.1.7"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist",
    "tag": "latest"
  }
}
