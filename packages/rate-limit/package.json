    "@techmely/types": "workspace:*",
    "tsup": "8.0.2",
    "valibot": "^0.28.1"
  },
  "funding": [
    {
      "type": "ko-fi",
      "url": "https://ko-fi.com/harrytran998"
    },
    {
      "type": "patreon",
      "url": "https://www.patreon.com/harrytran998"
