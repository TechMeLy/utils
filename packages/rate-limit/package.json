  "license": "MIT",
  "repository": "https://github.com/techmely/essential-packages/tree/dev/packages/types",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "publishConfig": {
    "access": "public",
    "directory": "dist",
    "tag": "latest"
  },
  "files": [
