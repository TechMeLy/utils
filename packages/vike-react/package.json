    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-streaming": "0.3.19",
    "tsup": "8.0.1",
    "vike": "^0.4.159"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist",
