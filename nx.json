{
  "extends": "@nrwl/workspace/presets/npm.json",
  "npmScope": "techmely",
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/nx-cloud",
      "options": {
        "cacheableOperations": ["build", "test", "lint", "package", "prepare"],
        "accessToken": "YjE4M2E3NjMtODMxMS00NmM1LThkNzctYmI2OWRlYTc3ZmVjfHJlYWQtd3JpdGU=",
        "showUsageWarnings": true,
        "cacheDirectory": "/tmp/nx-cache"
      }
    }
  },
  "targetDependencies": {
    "build": [
      {
        "target": "build",
        "projects": "dependencies"
      }
    ],
    "prepare": [
      {
        "target": "prepare",
        "projects": "dependencies"
      }
    ],
    "package": [
      {
        "target": "package",
        "projects": "dependencies"
      }
    ]
  },
  "projects": {
    "@techmely/types": {
      "implicitDependencies": []
    },
    "@techmely/utils": {
      "implicitDependencies": []
    }
  },
  "affected": {
    "defaultBase": "main"
  }
}
