{
  "name": "@techmely/vike-react",
  "version": "0.1.20",
  "homepage": "https://techmely.com/libraries/vike-react",
  "bugs": {
    "url": "https://github.com/techmely/essential-packages/issues"
  },
  "repository": {
    "url": "git+https://github.com/techmely/essential-packages.git#main"
  },
  "license": "MIT",
  "type": "module",
  "main": "index.js",
  "types": "index.d.ts",
  "files": ["dist", "CHANGELOG.md"],
  "scripts": {
    "clean": "rimraf dist",
    "upgrade.deps": "npm-check-updates -u",
    "build": "tsup --entry src/+config.h.ts --entry src/index.ts --entry src/useData.ts --entry src/usePageContext.ts --entry src/onRenderClient.tsx --entry src/onRenderHtml.tsx --sourcemap --clean --dts --format esm && bun fix.build",
    "fix.build": "mv dist/_config.h.d.ts dist/+config.h.d.ts",
    "publish": "node ../../scripts/publishPkg.mjs --only-esm"
  },
  "peerDependencies": {
    "@techmely/utils": "^2.1.18",
    "react": "18.x.x",
    "react-dom": "18.x.x",
    "react-streaming": "^0.3.22",
    "vike": "^0.4.153"
  },
  "devDependencies": {
    "@techmely/utils": "workspace:*",
    "@types/react": "18.2.78",
    "@types/react-dom": "^18.2.25",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-streaming": "0.3.24",
    "tsup": "8.0.2",
    "typescript": "5.4.5",
    "vike": "^0.4.168"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist",
    "tag": "latest"
  },
  "funding": [
    {
      "type": "ko-fi",
      "url": "https://ko-fi.com/techmely"
    },
    {
      "type": "patreon",
      "url": "https://www.patreon.com/techmely"
    }
  ],
  "keywords": ["techmely", "techmely-react", "vike react"]
}
