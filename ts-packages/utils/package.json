{
  "name": "@techmely/utils",
  "version": "3.0.2",
  "description": "Collection of helpful JavaScript / TypeScript utils",
  "author": "Harry Tran <nhattq.coding@gmail.com>",
  "license": "MIT",
  "type": "module",
  "repository": "https://github.com/techmely/essential-packages/tree/dev/ts-packages/utils",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "publishConfig": {
    "access": "public",
    "directory": "dist",
    "tag": "latest"
  },
  "files": ["dist", "package.json", "README.md"],
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./index.d.ts",
        "default": "./index.js"
      }
    },
    "./id": {
      "import": {
        "types": "./id.d.ts",
        "default": "./id.js"
      }
    },
    "./copyDir": {
      "import": {
        "types": "./copyDir.d.ts",
        "default": "./copyDir.js"
      }
    },
    "./emptyDir": {
      "import": {
        "types": "./emptyDir.d.ts",
        "default": "./emptyDir.js"
      }
    },
    "./findNearestFile": {
      "import": {
        "types": "./findNearestFile.d.ts",
        "default": "./findNearestFile.js"
      }
    },
    "./getDataPath": {
      "import": {
        "types": "./getDataPath.d.ts",
        "default": "./getDataPath.js"
      }
    },
    "./isFileReadable": {
      "import": {
        "types": "./isFileReadable.d.ts",
        "default": "./isFileReadable.js"
      }
    },
    "./isStream": {
      "import": {
        "types": "./isStream.d.ts",
        "default": "./isStream.js"
      }
    },
    "./mergeStreams": {
      "import": {
        "types": "./mergeStreams.d.ts",
        "default": "./mergeStreams.js"
      }
    },
    "./readFile": {
      "import": {
        "types": "./readFile.d.ts",
        "default": "./readFile.js"
      }
    },
    "./getAppVersion": {
      "import": {
        "types": "./getAppVersion.d.ts",
        "default": "./getAppVersion.js"
      }
    },
    "./getCurrentGitBranch": {
      "import": {
        "types": "./getCurrentGitBranch.d.ts",
        "default": "./getCurrentGitBranch.js"
      }
    },
    "./getGitLastCommitHash": {
      "import": {
        "types": "./getGitLastCommitHash.d.ts",
        "default": "./getGitLastCommitHash.js"
      }
    },
    "./getGitTags": {
      "import": {
        "types": "./getGitTags.d.ts",
        "default": "./getGitTags.js"
      }
    },
    "./getLastGitTags": {
      "import": {
        "types": "./getLastGitTags.d.ts",
        "default": "./getLastGitTags.js"
      }
    },
    "./writeFile": {
      "import": {
        "types": "./writeFile.d.ts",
        "default": "./writeFile.js"
      }
    }
  },
  "typesVersions": {
    "*": {}
  },
  "peerDependencies": {
    "dayjs": "^1.11.11"
  },
  "peerDependenciesMeta": {
    "dayjs": {
      "optional": true
    }
  },
  "optionalDependencies": {
    "nanoid": "^5.0.7"
  },
  "devDependencies": {
    "@techmely/types": "workspace:*",
    "@testing-library/jest-dom": "6.4.2",
    "@types/node": "20.12.7",
    "chalk": "^5.3.0",
    "dayjs": "^1.11.11",
    "execa": "^8.0.1",
    "nanoid": "^5.0.7",
    "time-span": "5.1.0",
    "tslib": "^2.6.2",
    "tsup": "8.0.2",
    "typescript": "5.4.5",
    "vitest": "1.5.2"
  },
  "funding": [
    {
      "type": "ko-fi",
      "url": "https://ko-fi.com/harrytran998"
    },
    {
      "type": "patreon",
      "url": "https://www.patreon.com/harrytran998"
    }
  ],
  "keywords": ["techmely", "techmely-utils"]
}
