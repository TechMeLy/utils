    "execa": "^8.0.1",
    "nanoid": "^5.0.7",
    "time-span": "5.1.0",
    "tslib": "^2.6.2",
    "tsup": "8.0.2",
    "typescript": "5.4.5",
    "vitest": "1.5.0"
