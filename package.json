{
  "name": "template-template-monorepo-typescript",
  "version": "0.0.1",
  "description": "",
  "author": "Harry Tran <nhattq.coding@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/techmely/products.git"
  },
  "workspaces": [
    "packages/**"
  ],
  "scripts": {
    "dev:web": "yarn workspace @techmely/web-news run dev",
    "build:web": "yarn workspace @techmely/web-news run build",
    "build:admin": "yarn workspace @techmely/admin run build",
    "preinstall": "npx only-allow yarn",
    "postinstall": "make git-hook",
    "lint:files": "ls-lint",
    "lint:eslint": "eslint --cache --ext .ts . --ignore-path .eslintignore",
    "lint:eslint:quiet": "eslint --cache --ext .ts . --ignore-path .eslintignore --quiet",
    "lint:eslint:fix": "eslint --cache --ext .ts . --ignore-path .eslintignore --fix",
    "test:jest": "jest --passWithNoTests",
    "test:jest:silent": "jest --silent --passWithNoTests",
    "release": "npx bumpp --commit --push --tag && npm publish",
    "clean": "yarn workspaces foreach -Apivt run clean",
    "clean:cache": "yarn cache clean --all && rimraf .yarn/build-state.yml .yarn/install-state.gz node_modules yarn.lock"
  },
  "devDependencies": {
    "@cucumber/cucumber": "8.0.0-rc.1",
    "@ls-lint/ls-lint": "1.10.0",
    "@sucrase/jest-plugin": "2.2.0",
    "@techmely/types": "1.0.1",
    "@types/eslint": "8.2.1",
    "@types/jest": "27.0.3",
    "@types/node": "17.0.4",
    "@typescript-eslint/eslint-plugin": "5.8.0",
    "@typescript-eslint/parser": "5.8.0",
    "@yarnpkg/sdks": "2.5.1-rc.6",
    "cross-env": "7.0.3",
    "cypress": "9.2.0",
    "cypress-cucumber-preprocessor": "4.3.1",
    "cypress-plugin-tab": "1.0.5",
    "eslint": "8.5.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-import-resolver-node": "^0.3.6",
    "eslint-plugin-cypress": "2.12.1",
    "eslint-plugin-import": "2.25.3",
    "eslint-plugin-jest": "25.3.0",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-promise": "6.0.0",
    "eslint-plugin-react": "7.28.0",
    "eslint-plugin-react-hooks": "4.3.0",
    "eslint-plugin-unicorn": "39.0.0",
    "jest": "27.4.5",
    "jscpd": "3.4.2",
    "prettier": "2.5.1",
    "rimraf": "3.0.2",
    "sucrase": "3.20.3",
    "turbo": "1.0.19",
    "typescript": "4.5.4"
  },
  "engines": {
    "node": ">=16.13.0",
    "npm": ">=8.1.0"
  },
  "packageManager": "yarn@3.1.1",
  "dependenciesMeta": {
    "react-dom@17.0.2": {
      "unplugged": true
    }
  }
}
