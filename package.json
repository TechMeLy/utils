{
  "name": "@techmely/products",
  "description": "",
  "author": "Harry Tran <nhattq.coding@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/techmely/products.git"
  },
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "postinstall": "make git-hook",
    "clean:modules": "npx rimraf node_modules packages/**/node_modules pnpm-lock.yaml",
    "dev:news": "turbo run dev --scope=@techmely/news --no-cache",
    "build:news": "turbo run build --scope=@techmely/news",
    "dev:admin": "turbo run dev --scope=@techmely/news-admin --no-cache",
    "build:admin": "turbo run build --scope=@techmely/news-admin",
    "start:admin": "pnpm --filter @techmely/news-admin run serve",
    "dev:core": "turbo run dev --scope=@techmely/core --no-cache",
    "build:core": "turbo run build --scope=@techmely/core",
    "lint:files": "ls-lint",
    "lint:eslint": "eslint --cache --ext .ts . --ignore-path .eslintignore",
    "lint:eslint:quiet": "eslint --cache --ext .ts . --ignore-path .eslintignore --quiet",
    "lint:eslint:fix": "eslint --cache --ext .ts . --ignore-path .eslintignore --fix",
    "test:unit": "vitest",
    "test:unit:coverage": "vitest --coverage",
    "test:e2e": "CYPRESS_baseUrl=http://localhost:3000 cypress open",
    "test:e2e:headless": "CYPRESS_baseUrl=http://localhost:3000 cypress run",
    "prettier": "prettier --write \"**/*.{js,ts,tsx,md}\""
  },
  "devDependencies": {
    "@cucumber/cucumber": "8.0.0-rc.1",
    "@ls-lint/ls-lint": "1.10.0",
    "@techmely/types": "1.0.4",
    "@types/eslint": "8.2.1",
    "@types/node": "17.0.5",
    "@typescript-eslint/eslint-plugin": "5.8.0",
    "@typescript-eslint/parser": "5.8.0",
    "c8": "7.10.0",
    "cross-env": "7.0.3",
    "cypress": "9.2.0",
    "cypress-cucumber-preprocessor": "4.3.1",
    "cypress-plugin-tab": "1.0.5",
    "eslint": "8.5.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-import-resolver-node": "^0.3.6",
    "eslint-plugin-cypress": "2.12.1",
    "eslint-plugin-import": "2.25.3",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-promise": "6.0.0",
    "eslint-plugin-react": "7.28.0",
    "eslint-plugin-react-hooks": "4.3.0",
    "eslint-plugin-unicorn": "39.0.0",
    "jscpd": "3.4.2",
    "jsdom": "19.0.0",
    "prettier": "2.5.1",
    "rimraf": "3.0.2",
    "turbo": "1.0.23",
    "typescript": "4.5.4",
    "vite": "2.7.7",
    "vitest": "0.0.115"
  },
  "engines": {
    "node": ">=16.13.0",
    "npm": ">=8.1.0"
  },
  "packageManager": "pnpm@6.24.2",
  "turbo": {
    "baseBranch": "origin/main",
    "pipeline": {
      "build": {
        "dependsOn": [
          "^build"
        ],
        "outputs": [
          ".next/**",
          "dist/**",
          "**/dist/**"
        ]
      },
      "test": {
        "dependsOn": [
          "^build"
        ],
        "outputs": []
      },
      "lint": {
        "outputs": []
      },
      "dev": {
        "cache": false
      }
    }
  }
}
